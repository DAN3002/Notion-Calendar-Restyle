{
	"manifest_version": 3,
	"name": "Notion Calendar Restyle",
	"version": "0.1.0",
	"description": "Injects custom CSS into Notion Calendar pages.",
	"content_scripts": [
		{
			"matches": [
				"https://calendar.notion.so/*",
				"https://calendar.notion.com/*"
			],
			"js": [
				"./scripts/content/content.js"
			],
			"css": [
				"./styles/styles.css"
			],
			"run_at": "document_end"
		}
	]
} 
